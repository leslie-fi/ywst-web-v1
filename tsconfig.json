{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
    "@components/*": ["./components/*"],
    "@lib/*": ["./lib/*"],
    "@pages/*": ["./pages/*"],
    "@types/*": ["./types/*"],
    "@styles/*": ["./styles/*"],
    "@utils/*": ["./utils/*"],
    }
  },
  "exclude": ["node_modules", "../ywst-studio-v1"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
